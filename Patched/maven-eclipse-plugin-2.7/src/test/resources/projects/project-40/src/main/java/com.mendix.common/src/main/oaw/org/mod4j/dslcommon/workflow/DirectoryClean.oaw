<?xml version="1.0" encoding="windows-1252"?>

<workflow>

    <!-- 
         Initialize the issues reporting feature of oAW
    -->
    <component class="org.eclipse.xtend.util.stdlib.ExtIssueReporter"/>

    <!--
         Cleaning of output directories.
    -->
    <component id="DirCleaner" class="org.eclipse.emf.mwe.utils.DirectoryCleaner">
        <directory value="${directories}"/>
    </component>
 
</workflow>