import BusinessDomainDsl;

BusinessDomainDsl::UniqueRule uniqueRule (BusinessDomainDsl::Property property) :

    ((BusinessDomainDsl::BusinessClass)property.eContainer).businessRules.typeSelect(BusinessDomainDsl::UniqueRule).select(e|e.properties.contains(property)).first();